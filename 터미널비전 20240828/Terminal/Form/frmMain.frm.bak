Private Sub WinsockPLC_DataArrival(ByVal bytesTotal As Long)
Dim sPLCData As String
Dim tempstr As String
Dim sTotStr(0 To 3) As String
Dim tempLen As Integer
Dim tempbyt As Byte
Dim stemp As String
Dim itemp As Integer
Dim sOnetemp(0 To 99) As String
Dim i As Integer
Dim j As Integer

    Select Case sWriteReadPLC
        Case "O"     'write
            WinsockPLC.GetData sPLCData
'            Me.txtTestData.Text = sPLCData
            Me.shpIOCheck(0).BackColor = vbWhite
        Case "I"     'read
            WinsockPLC.GetData sPLCData
            
            'testet = WinsockPLC.BytesReceived
            tempstr = Mid(sPLCData, 23, 4)
'            ListBox_Append Time & "  " & "PLC" & "에서  " & tempstr & " 를 받았습니다.", 0
'            ListBox_Append Time & "  " & WinsockPLC.State & " 를 받았습니다.", 0
            'Me.shpIOCheck(0).BackColor = vbRed
            If tempstr = "0002" Then   '0002 = 16비트로 0000 0000 0000 0010 임
                bTriggerOn = True      '트리거 On
                ListBox_Append Time & "  " & "PLC" & "에서  " & tempstr & " 를 받았습니다.", 0
                'ListBox_Append Time & "  " & WinsockPLC.State & " 를 받았습니다.", 0
            Else
                bTriggerOn = False
            End If
        '양승조 추가//////////////////////////
        Case "SpecChange"
            WinsockPLC.GetData sPLCData
            tempstr = Mid(sPLCData, 23, 4)
            
            If tempstr = "0001" Then   '0002 = 16비트로 0000 0000 0000 0001 임
                bSpecChangeOn = True      'SpecChange On
                ListBox_Append Time & "  " & "PLC" & "에서  " & tempstr & " 를 받았습니다.", 0
            Else
                bSpecChangeOn = False
            End If
        Case "JobChange"
            WinsockPLC.GetData sPLCData
            tempstr = Mid(sPLCData, 23, 4)
            
            '1이 있는 위치가 모델번호이다.
            Dim tempint As Integer
            tempstr = DJ_HexToBin(tempstr)
            tempint = 16 - (InStr(1, tempstr, "1") - 1)
            
            If nPreJobNum <> tempint Then   '0001 = 16비트로 0000 0000 0000 0001 임
                bJobChangeOn = True      'SpecChang On
                nPreJobNum = CInt(tempint)
                ListBox_Append Time & "  " & "PLC" & "에서  " & tempstr & " 를 받았습니다.", 0
            Else
                bJobChangeOn = False
            End If
        '양승조 추가//////////////////////////end
        Case "D"     'readData
            WinsockPLC.GetData sPLCData
            For i = 0 To 3
                sIDCode(i) = ""
            Next i
            tempLen = Len(sPLCData)                                 '이곳 TempLen 은 여기서쓰고 아래 내려가서는 지워져도 된다.
            tempstr = Mid(sPLCData, 23, (tempLen - 22))
            For j = 0 To 3
                sTotStr(j) = Trim(DJ_DataMake(tempstr, j))              'Hex -> 아스키로..
                tempLen = Len(sTotStr(j))
                For i = 1 To tempLen
                    sOnetemp(i) = Mid(sTotStr(j), i, 1)
                    If sOnetemp(i) = Chr(10) Or sOnetemp(i) = Chr(13) Then
                        sOnetemp(i) = ""
                    End If
                    sIDCode(j) = sIDCode(j) & sOnetemp(i)
                Next i
                
                If sIDCode(j) = "" Or Len(sIDCode(j)) < 8 Then     '8 은 아무 이유없어
                    sIDCode(j) = "NOID"
                End If
            Next j
            'Me.shpIOCheck(0).BackColor = vbWhite
            ListBox_Append Time & "  " & "PLC" & "에서  " & sIDCode(0) & " 를 받았습니다." & "(" & Len(sIDCode(0)) & ")", 0
            'Me.txtTestData.Text = sIDCode(0)
            Me.lblIDCodeNum(0).Caption = sIDCode(0)
            Me.lblIDCode.Caption = sIDCode(0)
        
        '양승조 추가//////////////////////////
        Case "ReadSpecTer"
            WinsockPLC.GetData sPLCData
            tempLen = Len(sPLCData)                                 '이곳 TempLen 은 여기서쓰고 아래 내려가서는 지워져도 된다.
            tempstr = Mid(sPLCData, 23, (tempLen - 22))

            For i = 0 To 4
                dSpecOri(i) = "&H" & Left(Mid(tempstr, (i * 24) + 1, 8), 4)
                dSpecMax(i) = "&H" & Left(Mid(tempstr, (i * 24) + 9, 8), 4)
                dSpecMin(i) = "&H" & Left(Mid(tempstr, (i * 24) + 17, 8), 4)
                
                dSpecOri(i) = dSpecOri(i) * 0.1
                dSpecMax(i) = dSpecMax(i) * 0.1
                dSpecMin(i) = dSpecMin(i) * 0.1
                
                dSpecOriMin(i) = dSpecOri(i) - dSpecMin(i)
                dSpecOriMax(i) = dSpecOri(i) + dSpecMax(i)
            Next i
            
        Case "ReadSpecAlg"
            WinsockPLC.GetData sPLCData
            tempLen = Len(sPLCData)                                 '이곳 TempLen 은 여기서쓰고 아래 내려가서는 지워져도 된다.
            tempstr = Mid(sPLCData, 23, (tempLen - 22))

            For i = 0 To 3
                dSpecOri(i) = "&H" & Left(Mid(tempstr, (i * 24) + 1, 8), 4)
                dSpecMax(i) = "&H" & Left(Mid(tempstr, (i * 24) + 9, 8), 4)
                dSpecMin(i) = "&H" & Left(Mid(tempstr, (i * 24) + 17, 8), 4)
                
                dSpecOri(i) = dSpecOri(i) * 0.1
                dSpecMax(i) = dSpecMax(i) * 0.1
                dSpecMin(i) = dSpecMin(i) * 0.1
                
                dSpecOriMin(i) = dSpecOri(i) - dSpecMin(i)
                dSpecOriMax(i) = dSpecOri(i) + dSpecMax(i)
            Next i
        '양승조 추가//////////////////////////end
    End Select
    bDArrival = True
    'sWriteReadPLC = ""
End Sub

